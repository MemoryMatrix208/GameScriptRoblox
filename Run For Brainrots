local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/WhoGodsyoure/MyHomeObf/refs/heads/main/Meow"))()

local Window = Library:CreateWindow({
    Title = "Run For Brainrots | ArchiveXNova",
    Accent = Color3.fromRGB(255, 100, 100)
})

local Main = Window:Section("Brainrot")

Main:Button("Auto Celestial", function()

    local player = game.Players.LocalPlayer
    local char = player.Character or player.CharacterAdded:Wait()
    local hrp = char:WaitForChild("HumanoidRootPart")

    
    hrp.CFrame = CFrame.new(9.241, 19.238, 4139.798)
    task.wait(1)

    
    local item = workspace:WaitForChild("ItemSpawners")
        :WaitForChild("Celestial")
        :WaitForChild("SpawnedItem")

    
    if item:IsA("Model") then
        hrp.CFrame = item:GetModelCFrame()
    else
        hrp.CFrame = item.CFrame
    end

    task.wait(1)

    
    for _, v in ipairs(item:GetDescendants()) do
        if v:IsA("ProximityPrompt") then
            fireproximityprompt(v)
            break
        end
    end

    task.wait(1)

    
    hrp.CFrame = CFrame.new(-20.592, 19.225, -448.899)

end)

