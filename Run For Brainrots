local Main = Window:Section("Money")

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")

local Player = Players.LocalPlayer
local loopConn
local mode = 1

local function collectAll()
    local character = Player.Character or Player.CharacterAdded:Wait()
    local hrp = character:WaitForChild("HumanoidRootPart")

    local plot = workspace:FindFirstChild("Plot_" .. Player.Name)
    if not plot then return end

    local floor = plot:FindFirstChild("Floor1")
    if not floor then return end

    local slots = floor:FindFirstChild("Slots")
    if not slots then return end

    for _, slot in pairs(slots:GetChildren()) do
        local collect = slot:FindFirstChild("CollectTouch")
        if collect and collect:IsA("BasePart") then
            firetouchinterest(hrp, collect, 0)
            firetouchinterest(hrp, collect, 1)
        end
    end
end

Main:Button("Auto Collect Money", function()

    if mode == 1 then
        collectAll()
        mode = 2

    elseif mode == 2 then
        if loopConn then
            loopConn:Disconnect()
        end

        loopConn = RunService.Heartbeat:Connect(function()
            collectAll()
        end)

        mode = 3

    elseif mode == 3 then
        if loopConn then
            loopConn:Disconnect()
            loopConn = nil
        end

        mode = 1
    end

end)

local Main = Window:Section("Upgrade Shop")

local ReplicatedStorage = game:GetService("ReplicatedStorage")

Main:Button("Purchase Speed +1", function()
    ReplicatedStorage.Events.PurchaseSpeed:FireServer(1)
end)

Main:Button("Purchase Speed +5", function()
    ReplicatedStorage.Events.PurchaseSpeed:FireServer(5)
end)

Main:Button("Purchase Speed +10", function()
    ReplicatedStorage.Events.PurchaseSpeed:FireServer(10)
end)

Main:Button("Purchase Base Upgrade", function()
    ReplicatedStorage.Events.RequestBaseUpgrade:FireServer()
end)

Main:Button("Purchase Carry", function()
    ReplicatedStorage.Events.PurchaseCarry:FireServer()
end)

local Main = Window:Section("Rebirth")

Main:Button("Click", function()
    ReplicatedStorage.Events.RequestRebirth:FireServer()
end)

local Main = Window:Section("Inventory")

Main:Button("Sell Inventory", function()
    ReplicatedStorage.Events.RequestSell:FireServer("Inventory")
end)

local Main = Window:Section("Map")

local Players = game:GetService("Players")
local Player = Players.LocalPlayer

local function tp(pos)
    local character = Player.Character or Player.CharacterAdded:Wait()
    local hrp = character:WaitForChild("HumanoidRootPart")
    hrp.CFrame = CFrame.new(pos)
end

Main:Button("Home", function()
    tp(Vector3.new(-20.592, 19.225, -448.899))
end)

Main:Button("Celestial", function()
    tp(Vector3.new(9.241, 19.238, 4139.798))
end)

Main:Button("Secret", function()
    tp(Vector3.new(-18.433, 19.238, 3366.022))
end)

Main:Button("Mythical", function()
    tp(Vector3.new(16.656, 19.238, 2483.747))
end)

Main:Button("Legendary", function()
    tp(Vector3.new(4.321, 19.225, 1706.446))
end)
