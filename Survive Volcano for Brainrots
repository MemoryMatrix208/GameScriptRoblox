local Main = Window:Section("Speed Upgrade")

Main:Button("Buy 10", function()
    local Event = game:GetService("ReplicatedStorage").Events.PurchaseSpeed
    Event:FireServer(10)
end)

local Main = Window:Section("Sell Inventory")

Main:Button("Sell All", function()
    local Event = game:GetService("ReplicatedStorage").Events.RequestSell
    Event:FireServer("Inventory")
end)

local Main = Window:Section("Rebirth")

Main:Button("One Rebirth", function()
    local Event = game:GetService("ReplicatedStorage").Events.RequestRebirth
    Event:FireServer()
end)

local Main = Window:Section("Money")

Main:Button("Collect Money", function()
    local Players = game:GetService("Players")
    local player = Players.LocalPlayer
    local char = player.Character or player.CharacterAdded:Wait()
    local hrp = char:WaitForChild("HumanoidRootPart")

    local plot = workspace:FindFirstChild("Plot_" .. player.Name)
    if not plot or not plot:IsA("Model") then return end

    for i = 1, 3 do
        local floor = plot:FindFirstChild("Floor" .. i)
        if floor then
            local slots = floor:FindFirstChild("Slots")
            if slots then
                for _, slot in ipairs(slots:GetChildren()) do
                    local collect = slot:FindFirstChild("CollectTouch", true)
                    if collect then
                        local part = collect:FindFirstChildWhichIsA("BasePart", true)
                        if part then
                            firetouchinterest(hrp, part, 0)
                            firetouchinterest(hrp, part, 1)
                        end
                    end
                end
            end
        end
    end
end)
