local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/WhoGodsyoure/Home/refs/heads/main/Neko.lua"))()
local ThemeManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/WhoGodsyoure/Home/refs/heads/main/ThemeManager.lua"))()
local SaveManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/WhoGodsyoure/Home/refs/heads/main/SaveManager.lua"))()

local Window = Library:CreateWindow({
    Title = "ArchiveXNova",
    Footer = "Run From Brainrot Obby | YouTube: ArchiveXNova",
    Icon = 85025739024001,
    NotifySide = "Right",
    ShowCustomCursor = true,
    Center = true,
    AutoShow = true,
})

local Tabs = {
    Main = Window:AddTab("Main"),
    Player = Window:AddTab("Players"),
    ["UI Settings"] = Window:AddTab("Settings")
}

local Group = Tabs.Main:AddLeftGroupbox("Win")

Group:AddButton({
    Text = "Use This If Map Has Started",
    Func = function()
        loadstring(game:HttpGet("https://pastefy.app/EKvxLTpZ/raw"))()
    end
})

local Group = Tabs.Player:AddLeftGroupbox("Players")

Group:AddButton({
    Text = "Gravity Coil (ON/OFF)",
    Func = function()
        local player = game.Players.LocalPlayer
        local char = player.Character
        if not char then return end
        
        local hum = char:FindFirstChild("Humanoid")
        local hrp = char:FindFirstChild("HumanoidRootPart")
        if not hum or not hrp then return end
        
        local existing = hrp:FindFirstChild("GravityCoilForce")
        if existing then
            existing:Destroy()
        else
            local bf = Instance.new("BodyForce")
            bf.Name = "GravityCoilForce"
            bf.Force = Vector3.new(0, workspace.Gravity * hrp:GetMass() * 3, 0)
            bf.Parent = hrp
        end
    end
})

Group:AddButton({
    Text = "Speed Coil (ON/OFF)",
    Func = function()
        local player = game.Players.LocalPlayer
        local char = player.Character
        if not char then return end
        
        local hum = char:FindFirstChild("Humanoid")
        if not hum then return end
        
        if hum.WalkSpeed > 64 then
            hum.WalkSpeed = 64
        else
            hum.WalkSpeed = 64
        end
    end
})

local Group = Tabs.Main:AddLeftGroupbox("Obby")

local Players = game:GetService("Players")
local Player = Players.LocalPlayer

local function getHRP()
    local c = Player.Character or Player.CharacterAdded:Wait()
    return c:WaitForChild("HumanoidRootPart")
end

Group:AddButton({
    Text = "Click",
    Func = function()
        getHRP().CFrame = CFrame.new(-948.372, 71.649, 127.231)
    end
})

local UIGroup = Tabs["UI Settings"]

ThemeManager:SetLibrary(Library)
SaveManager:SetLibrary(Library)

SaveManager:IgnoreThemeSettings()
SaveManager:SetFolder("ArchiveXNova")
SaveManager:BuildConfigSection(Tabs["UI Settings"])
ThemeManager:ApplyToTab(Tabs["UI Settings"])
