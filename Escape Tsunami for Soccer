local Main = Window:Section("Money")

Main:Button("Auto Collect Money", function()
    local Event = game:GetService("ReplicatedStorage").Remotes.Plot.CollectCash

    for i = 1, 8 do
        Event:FireServer(i)
        task.wait(0.1)
    end
end)

local Main = Window:Section("Upgrade Shop")

Main:Button("Buy Speed +1", function()
    game:GetService("ReplicatedStorage")
        .Remotes.SpeedEvents.ExtraSpeed
        :FireServer(1)
end)

Main:Button("Buy Speed +5", function()
    game:GetService("ReplicatedStorage")
        .Remotes.SpeedEvents.ExtraSpeed
        :FireServer(5)
end)

Main:Button("Buy Speed +10", function()
    game:GetService("ReplicatedStorage")
        .Remotes.SpeedEvents.ExtraSpeed
        :FireServer(10)
end)

local Main = Window:Section("Rebirth")

Main:Button("Click", function()
    game:GetService("ReplicatedStorage")
        .Remotes.Plot.RebirthEvent
        :FireServer()
end)

local Main = Window:Section("Tsunami")

Main:Button("Remove Tsunami", function()
    local tsunami = workspace.Map:FindFirstChild("Tsunami")
    if tsunami then
        tsunami:Destroy()
    end
end)

local Main = Window:Section("VIP")

Main:Button("Remove Door", function()
    local vipZones = workspace.Map.Zones:FindFirstChild("VipZones")
    if vipZones then
        vipZones:Destroy()
    end
end)
