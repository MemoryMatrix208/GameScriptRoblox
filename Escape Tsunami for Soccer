local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/WhoGodsyoure/MyHome/refs/heads/main/Made%20By%20Me%20Part%202%20Open%20And%20Close"))()

local Window = Library:CreateWindow({
    Title = "Escape Tsunami for Soccer | ArchiveXNova"
})

local Main = Window:Section("Soccer")

Main:Button("Automatic Pick Up Secret", function()
    local player = game.Players.LocalPlayer
    local char = player.Character or player.CharacterAdded:Wait()
    local hrp = char:WaitForChild("HumanoidRootPart")

    local returnPos = Vector3.new(29.715, 4.225, 8.817)

    local npcFolder = workspace.Map
        .Zones
        .ByRarity
        .Secret
        .NPC

    local npcModel = npcFolder:FindFirstChildWhichIsA("Model")
    if not npcModel then return end

    local part = npcModel.PrimaryPart or npcModel:FindFirstChildWhichIsA("BasePart")
    if not part then return end

    hrp.CFrame = part.CFrame + Vector3.new(0, 3, 0)
    task.wait(0.3)

    for _, v in ipairs(npcModel:GetDescendants()) do
        if v:IsA("ProximityPrompt") then
            fireproximityprompt(v)
        end
    end

    task.wait(0.3)
    hrp.CFrame = CFrame.new(returnPos)
end)

