local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/WhoGodsyoure/MyHomeObf/refs/heads/main/Meow"))()

local Window = Library:CreateWindow({
    Title = "Speed Coil Escape | ArchiveXNova",
    Accent = Color3.fromRGB(255, 100, 100)
})

local Main = Window:Section("Win")

local mode = 0
local running = false

Main:Button("Auto Win", function()
    mode = mode + 1
    if mode > 3 then mode = 1 end

    local player = game.Players.LocalPlayer

    if mode == 1 then
        running = false
        local char = player.Character or player.CharacterAdded:Wait()
        local hrp = char:FindFirstChild("HumanoidRootPart")
        if hrp then
            hrp.CFrame = CFrame.new(4963.181, 116.031, 30.163)
        end

    elseif mode == 2 then
        running = true
        task.spawn(function()
            while running do
                local char = player.Character
                local hrp = char and char:FindFirstChild("HumanoidRootPart")
                if hrp then
                    hrp.CFrame = CFrame.new(4963.181, 116.031, 30.163)
                end
                task.wait()
            end
        end)

    elseif mode == 3 then
        running = false
    end
end)
