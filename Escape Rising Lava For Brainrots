local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/WhoGodsyoure/MyHomeObf/refs/heads/main/Meow"))()

local Window = Library:CreateWindow({
    Title = "Escape Rising Lava For Brainrots | ArchiveXNova",
    Accent = Color3.fromRGB(255, 100, 100)
})

local Main = Window:Section("Brainrots")

Main:Button("Auto HellFire", function()

    local player = game.Players.LocalPlayer
    local char = player.Character or player.CharacterAdded:Wait()
    local hrp = char:WaitForChild("HumanoidRootPart")

    local zone = workspace:WaitForChild("Brainrots")
        :WaitForChild("Zone9")
        :WaitForChild("B33")

    hrp.CFrame = zone:GetModelCFrame()

    task.wait(1)

    for _, v in ipairs(zone:GetDescendants()) do
        if v:IsA("ProximityPrompt") then
            fireproximityprompt(v)
        end
    end

    task.wait(1)

    hrp.CFrame = CFrame.new(69.462, -4.155, -196.224)

end)

Main:Button("Auto Ethereal", function()

    local player = game.Players.LocalPlayer
    local char = player.Character or player.CharacterAdded:Wait()
    local hrp = char:WaitForChild("HumanoidRootPart")

    local zone = workspace:WaitForChild("Brainrots")
        :WaitForChild("Zone8")

    if zone:IsA("Model") then
        hrp.CFrame = zone:GetModelCFrame()
    else
        local part = zone:FindFirstChildWhichIsA("BasePart", true)
        if part then
            hrp.CFrame = part.CFrame
        end
    end

    task.wait(1)

    for _, v in ipairs(zone:GetDescendants()) do
        if v:IsA("ProximityPrompt") then
            fireproximityprompt(v)
            break
        end
    end

    task.wait(1)

    hrp.CFrame = CFrame.new(69.462, -4.155, -196.224)

end)

