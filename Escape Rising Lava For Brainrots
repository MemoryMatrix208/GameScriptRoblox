local Main = Window:Section("Money")

local mode = 1
local running = false

Main:Button("Auto Collect Money", function()

    local player = game.Players.LocalPlayer
    local char = player.Character or player.CharacterAdded:Wait()
    local hrp = char:WaitForChild("HumanoidRootPart")
    local plots = workspace:WaitForChild("Plots")

    local function collect()
        for i = 1, 5 do
            local plot = plots:FindFirstChild(tostring(i))
            if plot then
                local collectFolder = plot:FindFirstChild("Collect")
                if collectFolder then
                    for j = 1, 10 do
                        local collectModel = collectFolder:FindFirstChild(tostring(j))
                        if collectModel then
                            local button = collectModel:FindFirstChild("Button", true)
                            if button and button:IsA("BasePart") then
                                firetouchinterest(hrp, button, 0)
                                firetouchinterest(hrp, button, 1)
                            end
                        end
                    end
                end
            end
        end
    end

    if mode == 1 then
        collect()
        mode = 2

    elseif mode == 2 then
        if running then return end
        running = true

        task.spawn(function()
            while running do
                collect()
                task.wait(0.5)
            end
        end)

        mode = 3

    elseif mode == 3 then
        running = false
        mode = 1
    end

end)

local Main = Window:Section("Upgrade Shop")

Main:Button("Purchase +1 Speed", function()
    game:GetService("ReplicatedStorage")
        .Communication.Events:GetChildren()[15]
        :FireServer("First")
end)

Main:Button("Purchase +5 Speed", function()
    game:GetService("ReplicatedStorage")
        .Communication.Events:GetChildren()[15]
        :FireServer("Second")
end)

Main:Button("Purchase +10 Speed", function()
    game:GetService("ReplicatedStorage")
        .Communication.Events:GetChildren()[15]
        :FireServer("Third")
end)

Main:Button("Purchase Holding", function()
    game:GetService("ReplicatedStorage")
        .Communication.Events:GetChildren()[19]
        :FireServer("First")
end)

Main:Button("Purchase Base Upgrade", function()
    game:GetService("ReplicatedStorage")
        .Communication.Functions:GetChildren()[9]
        :InvokeServer()
end)

local Main = Window:Section("Rebirth")

Main:Button("Click", function()
    game:GetService("ReplicatedStorage")
        .Communication.Events:GetChildren()[10]
        :FireServer()
end)

local Main = Window:Section("Lava")

Main:Button("Remove Lava", function()
    local lava = workspace:FindFirstChild("GameLava")
    if lava then
        lava:Destroy()
    end
end)

local Main = Window:Section("Map")

Main:Button("TP Home", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame =
        CFrame.new(69.462, -4.155, -196.224)
end)

