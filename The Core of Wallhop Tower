local LoopConnection

MainSection:AddDropdown("EpicDropDownName", {
    Title = "Teleport",
    Description = "",
    Options = Options,
    Default = "",
    PlaceHolder = "Search Mode",
    Multiple = false,
    Callback = function(selected)
        local player = game:GetService("Players").LocalPlayer
        local hrp = player.Character and player.Character:FindFirstChild("HumanoidRootPart")
        if not hrp then return end

        if selected:find("^Just once") then
            hrp.CFrame = CFrame.new(TargetPosition)

        elseif selected:find("^Loop") then
            if LoopConnection then LoopConnection:Disconnect() end
            LoopConnection = game:GetService("RunService").Heartbeat:Connect(function()
                hrp.CFrame = CFrame.new(TargetPosition)
            end)

        elseif selected == "Stop Loop" then
            if LoopConnection then
                LoopConnection:Disconnect()
                LoopConnection = nil
            end

        elseif selected == "Stop Farm" then
            if LoopConnection then
                LoopConnection:Disconnect()
                LoopConnection = nil
            end

        elseif selected:find("^System | Emergency Stop") then
            if LoopConnection then
                LoopConnection:Disconnect()
                LoopConnection = nil
            end
        end
    end
})
