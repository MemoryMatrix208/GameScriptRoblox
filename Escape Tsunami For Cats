local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/WhoGodsyoure/MyHomeObf/refs/heads/main/Meow"))()

local Window = Library:CreateWindow({
    Title = "Escape Tsunami For Cats | ArchiveXNova",
    Accent = Color3.fromRGB(255, 100, 100)
})

local Main = Window:Section("Cats")

Main:Button("Auto OG", function()

    local player = game.Players.LocalPlayer
    local char = player.Character or player.CharacterAdded:Wait()
    local hrp = char:WaitForChild("HumanoidRootPart")

    local oldPos = hrp.CFrame

    hrp.CFrame = CFrame.new(-20.646, 3.225, 34.869)

    task.wait(1)

    local og = workspace:WaitForChild("ItemSpawners"):WaitForChild("OG")
    local item = og:FindFirstChild("SpawnedItem")

    if item then
        for _,v in pairs(item:GetDescendants()) do
            if v:IsA("ProximityPrompt") then
                fireproximityprompt(v)
                break
            end
        end
    end

    task.wait(1)

    hrp.CFrame = oldPos

end)

Main:Button("Auto Mythical", function()

    local player = game.Players.LocalPlayer
    local char = player.Character or player.CharacterAdded:Wait()
    local hrp = char:WaitForChild("HumanoidRootPart")

    local oldPos = hrp.CFrame

    hrp.CFrame = CFrame.new(-20.646, 3.225, 34.869)

    task.wait(1)

