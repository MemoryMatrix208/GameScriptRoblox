local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/WhoGodsyoure/Home/refs/heads/main/%E3%82%A2%E3%83%83%E3%83%A9%E3%83%BC%E4%BB%A5%E5%A4%96%E3%81%AB%E7%A5%9E%E3%81%AF%E3%81%84%E3%81%AA%E3%81%84"))()

local Window = Library:CreateWindow('ArchiveXNova') -- :CreateWindow(Title)

local Section = Window:Section('FIND THE BRAINROT MORPHS') -- :Section(Title)

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local Folder = workspace.Gameplay.CharacterPoints

local function getHRP()
    local char = LocalPlayer.Character
    return char and char:FindFirstChild("HumanoidRootPart")
end

local function touchAll()
    local hrp = getHRP()
    if not hrp then return end

    for _, model in pairs(Folder:GetChildren()) do
        if model:IsA("Model") then
            local part = model.PrimaryPart or model:FindFirstChildWhichIsA("BasePart", true)
            if part then
                firetouchinterest(hrp, part, 0)
                firetouchinterest(hrp, part, 1)
            end
        end
    end
end

Section:Button('Click', function()
    touchAll()
end)

Section:Label('Subscribe') -- :Label(Text)
