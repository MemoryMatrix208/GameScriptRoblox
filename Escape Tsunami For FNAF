local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/WhoGodsyoure/MyHomeObf/refs/heads/main/Meow"))()

local Window = Library:CreateWindow({
    Title = "Escape Tsunami For FNAF | ArchiveXNova",
    Accent = Color3.fromRGB(255, 100, 100)
})

local Main = Window:Section("Brainrot")

local Players = game:GetService("Players")
local Player = Players.LocalPlayer

local function tp(cf)
    local character = Player.Character or Player.CharacterAdded:Wait()
    local hrp = character:WaitForChild("HumanoidRootPart")
    hrp.CFrame = cf
end

Main:Button("Auto Secret", function()

    tp(CFrame.new(20.047, 3.225, 2836.468))

    task.wait(0.1)

    local secret = workspace:FindFirstChild("ItemSpawners")
        and workspace.ItemSpawners:FindFirstChild("Secret")

    if secret then
        local item = secret:FindFirstChild("SpawnedItem")
        if item then

            local targetCF

            if item:IsA("Model") then
                targetCF = item:GetPivot()
            elseif item:IsA("BasePart") then
                targetCF = item.CFrame
            end

            if targetCF then
                tp(targetCF + Vector3.new(0, 3, 0))
                task.wait(0.1)

                local prompt = item:FindFirstChildWhichIsA("ProximityPrompt", true)
                if prompt then
                    fireproximityprompt(prompt)
                end
            end
        end
    end

    task.wait(0.1)

    tp(CFrame.new(-29.614, 3.225, 41.489))

end)

