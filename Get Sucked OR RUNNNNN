local Main = main:AddTab("Area")
main:SelectTab()

local MainSection = Main:AddSection({Title = "Get Sucked OR RUNNNNN", Description = "Description", Defualt = false , Locked = false})
local GroupButtons =   MainSection:AddGroupButton()

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local Areas = {
    ["Area 1"] = Vector3.new(-1358.057, 3.197, 26.347),
    ["Area 2"] = Vector3.new(-1392.535, 3.197, -643.881),
    ["Area 3"] = Vector3.new(-1786.960, 3.197, -1100.930),
}

local function teleportTo(pos)
    local char = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
    local hrp = char:WaitForChild("HumanoidRootPart")
    hrp.CFrame = CFrame.new(pos)
end

MainSection:AddDropdown("EpicDropDownName", {
    Title = "Area",
    Description = "",
    Options = {
        "Area 1",
        "Area 2",
        "Area 3",
    },
    Default = "",
    PlaceHolder = "Search Name",
    Multiple = false,
    Callback = function(choice)
        local pos = Areas[choice]
        if pos then
            teleportTo(pos)
        end
    end
})
