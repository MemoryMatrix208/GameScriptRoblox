local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/WhoGodsyoure/Home/refs/heads/main/%E3%82%A2%E3%83%83%E3%83%A9%E3%83%BC%E4%BB%A5%E5%A4%96%E3%81%AB%E7%A5%9E%E3%81%AF%E3%81%84%E3%81%AA%E3%81%84"))()

local Window = Library:CreateWindow('ArchiveXNova') -- :CreateWindow(Title)

local Section = Window:Section('Checkpoints') -- :Section(Title)

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local function getHRP()
    local char = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
    return char:WaitForChild("HumanoidRootPart")
end

Section:Button("Teleport", function()
    for i = 1, 90 do
        local stage = workspace.Stages:FindFirstChild(tostring(i))
        if stage then
            local spawn = stage:FindFirstChild("Spawn")
            if spawn and spawn:IsA("BasePart") then
                getHRP().CFrame = spawn.CFrame + Vector3.new(0, 3, 0)
                task.wait(0.05)
            end
        end
    end
end)

Section:Label('Subscribe') -- :Label(Text)
