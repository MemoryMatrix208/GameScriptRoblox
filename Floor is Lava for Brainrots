local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/WhoGodsyoure/MyHome/refs/heads/main/Made%20By%20Me%20Part%202%20Open%20And%20Close"))()

local Window = Library:CreateWindow({
    Title = "Floor is Lava for Brainrots | ArchiveXNova"
})

local Main = Window:Section("Brainrots")

Main:Button("Automatic Pick Up Secret", function()
    local Players = game:GetService("Players")
    local p = Players.LocalPlayer
    local c = p.Character or p.CharacterAdded:Wait()
    local hrp = c:WaitForChild("HumanoidRootPart")

    local backPos = Vector3.new(814.096, 3.995, -556.721)

    local model = workspace
        :WaitForChild("Tunnel")
        :WaitForChild("Zone")
        :WaitForChild("Secret")
        :FindFirstChildWhichIsA("Model")

    if not model then return end

    local part = model.PrimaryPart or model:FindFirstChildWhichIsA("BasePart", true)
    if not part then return end

    
    hrp.CFrame = part.CFrame * CFrame.new(0, 7, 0)
    task.wait()

    
    local platform = Instance.new("Part")
    platform.Size = Vector3.new(8, 1, 8)
    platform.Anchored = true
    platform.CanCollide = true
    platform.Transparency = 1
    platform.Name = "TempPlatform"
    platform.CFrame = hrp.CFrame * CFrame.new(0, -3.5, 0)
    platform.Parent = workspace

    task.wait(0.15)

    
    local prompt = model:FindFirstChildWhichIsA("ProximityPrompt", true)
    if prompt then
        fireproximityprompt(prompt)
    end

    task.wait(0.25)

    
    platform:Destroy()

    
    hrp.CFrame = CFrame.new(backPos)
end)

