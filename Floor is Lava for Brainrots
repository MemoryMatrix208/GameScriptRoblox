local Main = Window:Section("Money")

Main:Button("Auto Collect Money", function()
    local p = game:GetService("Players").LocalPlayer
    local c = p.Character or p.CharacterAdded:Wait()
    local hrp = c:WaitForChild("HumanoidRootPart")

    for i = 1, 5 do
        local plot = workspace.Plots:FindFirstChild(tostring(i))
        if plot and plot:FindFirstChild("Slots") then
            for j = 1, 10 do
                local slot = plot.Slots:FindFirstChild(tostring(j))
                if slot and slot:FindFirstChild("Collect") then
                    local collect = slot.Collect

                    local hitbox = collect:FindFirstChild("Hitbox")
                    if hitbox and hitbox:IsA("BasePart") then
                        firetouchinterest(hrp, hitbox, 0)
                        firetouchinterest(hrp, hitbox, 1)
                    else
                        local part = collect:FindFirstChildWhichIsA("BasePart", true)
                        if part then
                            firetouchinterest(hrp, part, 0)
                            firetouchinterest(hrp, part, 1)
                        end
                    end
                end
            end
        end
    end
end)

local Main = Window:Section("VIP")

Main:Button("Remove Walls", function()
    local walls = workspace:FindFirstChild("Map")
        and workspace.Map:FindFirstChild("Walls")

    if walls then
        walls:Destroy()
    end
end)
