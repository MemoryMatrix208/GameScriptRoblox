local lib = loadstring(game:HttpGet("https://raw.githubusercontent.com/WhoGodsyoure/Home/refs/heads/main/%E4%BA%BA%E7%94%9F%E3%81%AE%E6%84%8F%E5%91%B3%E3%81%A8%E3%81%AF%E4%BD%95%E3%81%8B"))() 
local FlagsManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/WhoGodsyoure/Home/refs/heads/main/Configure"))()

local main = lib:Load({
      Title = 'ArchiveXNova',
      ToggleButton = "rbxassetid://85025739024001",
	  BindGui = Enum.KeyCode.RightControl,
  })

local Main = main:AddTab("Win")
main:SelectTab()

local MainSection = Main:AddSection({Title = "Find Brainrots Tower", Description = "Description", Defualt = false , Locked = false})
local GroupButtons =   MainSection:AddGroupButton()

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")

local Player = Players.LocalPlayer

local Points = {
    CFrame.new(484.335, 209.214, -322.741),
}

local function HRP()
    local c = Player.Character or Player.CharacterAdded:Wait()
    return c:WaitForChild("HumanoidRootPart")
end

local function killChar()
    local char = Player.Character
    if char then
        local humanoid = char:FindFirstChildOfClass("Humanoid")
        if humanoid then
            humanoid.Health = 0
        end
    end
    Player.CharacterAdded:Wait()
end

local LoopConnection
local Thread

local function stopAll()
    if LoopConnection then LoopConnection:Disconnect() LoopConnection = nil end
    if Thread then task.cancel(Thread) Thread = nil end
end

local function once(delay)
    stopAll()
    killChar()
    for _, cf in ipairs(Points) do
        if delay > 0 then task.wait(delay) end
        HRP().CFrame = cf
    end
end

local function loop(delay)
    stopAll()
    killChar()
    LoopConnection = RunService.Heartbeat:Connect(function()
        for _, cf in ipairs(Points) do
            HRP().CFrame = cf
            if delay > 0 then task.wait(delay) end
        end
    end)
end

local function auto(delay)
    stopAll()
    killChar()
    Thread = task.spawn(function()
        while true do
            for _, cf in ipairs(Points) do
                HRP().CFrame = cf
                if delay > 0 then task.wait(delay) end
            end
        end
    end)
end

local function farm(delay)
    stopAll()
    killChar()
    LoopConnection = RunService.Heartbeat:Connect(function()
        for _, cf in ipairs(Points) do
            HRP().CFrame = cf
            if delay > 0 then task.wait(delay) end
        end
    end)
end

local Modes = {
    ["Just once | Normal"] = function() once(0.5) end,
    ["Just once | Fast"] = function() once(0.25) end,
    ["Just once | Super Fast"] = function() once(0.1) end,
    ["Just once | Speed of light"] = function() once(0) end,
    ["Just once | Stable"] = function() once(0.4) end,
    ["Just once | Debug Mode"] = function() once(0) end,
    ["Just once | Lightning"] = function() once(0.05) end,
    ["Just once | Hyper Speed"] = function() once(0.03) end,
    ["Just once | Quantum"] = function() once(0.01) end,
    ["Just once | Instant"] = function() once(0) end,
    ["Just once | Immediate"] = function() once(0) end,
    ["Just once | Rapid"] = function() once(0.08) end,
    ["Just once | Swift"] = function() once(0.12) end,
    ["Just once | Quick"] = function() once(0.18) end,
    ["Just once | Express"] = function() once(0.15) end,
    ["Just once | Blitz"] = function() once(0.04) end,
    ["Just once | Flash"] = function() once(0.02) end,
    ["Just once | Sonic"] = function() once(0.06) end,
    ["Just once | Turbo"] = function() once(0.07) end,
    ["Just once | Extreme"] = function() once(0.02) end,
    ["Just once | Ultimate"] = function() once(0) end,
    ["Just once | Maximum"] = function() once(0) end,

    ["Loop | Fast"] = function() loop(0.3) end,
    ["Loop | Super Fast"] = function() loop(0.15) end,
    ["Loop | Speed of light"] = function() loop(0) end,
    ["Loop | Stable"] = function() loop(0.5) end,
    ["Loop | Infinite"] = function() loop(0) end,
    ["Stop Loop"] = function() stopAll() end,

    ["Auto | Smart Mode"] = function() auto(0.2) end,
    ["Auto | Extreme Autonomous"] = function() auto(0) end,

    ["Farm | Efficient"] = function() farm(0.4) end,
    ["Farm | Extreme Farming"] = function() farm(0) end,
    ["Stop Farm"] = function() stopAll() end,

    ["System | Emergency Stop"] = function() stopAll() end,
    ["System | Hard Reset"] = function() killChar() end,
}

local Options = {}
for k in pairs(Modes) do
    Options[#Options + 1] = k
end
table.sort(Options)

MainSection:AddDropdown("EpicDropDownName", {
    Title = "Win Mode",
    Description = "",
    Options = Options,
    Default = "",
    PlaceHolder = "Search Mode",
    Multiple = false,
    Callback = function(choice)
        local f = Modes[choice]
        if f then f() end
    end
})

local Main = main:AddTab("Brainrots")
main:SelectTab()

local MainSection = Main:AddSection({Title = "Find Brainrots Tower", Description = "Description", Defualt = false , Locked = false})
local GroupButtons =   MainSection:AddGroupButton()

GroupButtons:AddButton({
    Title = "Auto Find Brainrots",
    Callback = function()
        local Players = game:GetService("Players")
        local Player = Players.LocalPlayer

        while true do
            local Char = Player.Character or Player.CharacterAdded:Wait()
            local HRP = Char:WaitForChild("HumanoidRootPart")

            local Folder = workspace:WaitForChild("Tower"):WaitForChild("Brainrots")
            local found = false

            for _, part in ipairs(Folder:GetChildren()) do
                if part:IsA("BasePart") and part.Parent then
                    found = true
                    firetouchinterest(HRP, part, 0)
                    firetouchinterest(HRP, part, 1)
                    task.wait(0.03)
                end
            end

            if not found then
                task.wait(0.5)
            end
        end
    end,
})
