local lib = loadstring(game:HttpGet("https://raw.githubusercontent.com/WhoGodsyoure/Home/refs/heads/main/%E4%BA%BA%E7%94%9F%E3%81%AE%E6%84%8F%E5%91%B3%E3%81%A8%E3%81%AF%E4%BD%95%E3%81%8B"))() 
local FlagsManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/WhoGodsyoure/Home/refs/heads/main/Configure"))()

local main = lib:Load({
      Title = 'ArchiveXNova',
      ToggleButton = "rbxassetid://85025739024001",
	  BindGui = Enum.KeyCode.RightControl,
  })

local Main = main:AddTab("+1 Win")
main:SelectTab()

local MainSection = Main:AddSection({Title = "Size Troll Tower", Description = "Description", Defualt = false , Locked = false})
local GroupButtons =   MainSection:AddGroupButton()

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local LocalPlayer = Players.LocalPlayer

local TARGET_POS = Vector3.new(186.652, 325.667, -33.631)

local looping = false
local loopThread = nil

local speedDelay = {
    Normal = 0.5,
    Fast = 0.25,
    ["Super Fast"] = 0.1,
    ["Speed of light"] = 0,
    Stable = 0.4,
    ["Debug Mode"] = 1,

    Lightning = 0,
    ["Hyper Speed"] = 0,
    Quantum = 0,
    Instant = 0,
    Immediate = 0,
    Rapid = 0.05,
    Swift = 0.1,
    Quick = 0.15,
    Express = 0.08,
    Blitz = 0,
    Flash = 0,
    Sonic = 0,
    Turbo = 0,
    Extreme = 0,
    Ultimate = 0,
    Maximum = 0,
    Optimal = 0.2,
    Balanced = 0.3,
    Conservative = 0.6,
    Minimal = 0.8,
    Lite = 0.7,
    Standard = 0.5,
    Premium = 0.2,
    Professional = 0.15,
    Enterprise = 0.1,
    ["Pro Precision"] = 0.05,
    ["Pro Optimized"] = 0.05,
    ["Extreme Burst"] = 0,
    ["Extreme No Delay"] = 0,
}

local function getHRP()
    local char = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
    return char:WaitForChild("HumanoidRootPart")
end

local function teleportOnce(mode)
    local hrp = getHRP()
    hrp.CFrame = CFrame.new(TARGET_POS)

    if speedDelay[mode] == 0 then
        RunService.Heartbeat:Wait()
    else
        task.wait(speedDelay[mode] or 0.3)
    end
end

local function startLoop(mode)
    looping = true
    loopThread = task.spawn(function()
        while looping do
            teleportOnce(mode)
        end
    end)
end

local function stopLoop()
    looping = false
end

MainSection:AddDropdown("EpicDropDownName", {
    Title = "Teleport Mode",
    Description = "",
    Options = {

        -- JUST ONCE
        "Just once | Normal",
        "Just once | Fast",
