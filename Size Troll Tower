local Main = main:AddTab("Item")
main:SelectTab()

local MainSection = Main:AddSection({Title = "Size Troll Tower", Description = "Description", Defualt = false , Locked = false})
local GroupButtons =   MainSection:AddGroupButton()

local Players = game:GetService("Players")
local Player = Players.LocalPlayer

local function HRP()
    local c = Player.Character or Player.CharacterAdded:Wait()
    return c:WaitForChild("HumanoidRootPart")
end

MainSection:AddDropdown("TeleportDrop", {
    Title = "Location",
    Description = "",
    Options = {
        "Slap Red",
        "FusionCoil",
        "Banana",
        "Slap Blue"
    },
    Default = "",
    PlaceHolder = "Search Name",
    Multiple = false,
    Callback = function(v)
        if v == "Slap Red" then
            HRP().CFrame = CFrame.new(220.179, 4.547, 13.932)

        elseif v == "FusionCoil" then
            HRP().CFrame = CFrame.new(-23.553, 10.225, 222.399)

        elseif v == "Banana" then
            HRP().CFrame = CFrame.new(38.290, 54.530, 35.605)

        elseif v == "Slap Blue" then
            HRP().CFrame = CFrame.new(178.315, 323.500, -23.736)
        end
    end
})
