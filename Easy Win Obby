local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/WhoGodsyoure/Home/refs/heads/main/%E3%82%A2%E3%83%83%E3%83%A9%E3%83%BC%E4%BB%A5%E5%A4%96%E3%81%AB%E7%A5%9E%E3%81%AF%E3%81%84%E3%81%AA%E3%81%84"))()

local Window = Library:CreateWindow('ArchiveXNova') -- :CreateWindow(Title)

local Section = Window:Section('Easy Win Obby') -- :Section(Title)

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local function getHRP()
    local char = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
    return char:WaitForChild("HumanoidRootPart")
end

local checkpoints = {}

for i = 1, 59 do
    local part = workspace.Checkpoints:FindFirstChild(tostring(i))
    if part and part:IsA("MeshPart") then
        checkpoints[i] = part
    end
end

local index = 1

Section:Button('Teleport Checkpoint', function()
    local hrp = getHRP()
    local target = checkpoints[index]
    if target then
        local pos = target.Position
        hrp.CFrame = CFrame.new(pos.X, pos.Y, pos.Z)
        index += 1
        if index > 59 then
            index = 1
        end
    end
end)

Section:Label('Subscribe') -- :Label(Text)
