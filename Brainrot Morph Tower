local Main = main:AddTab("Playtime Rewards")
main:SelectTab()

local MainSection = Main:AddSection({Title = "Brainrot Morph Tower", Description = "Description", Defualt = false , Locked = false})
local GroupButtons =   MainSection:AddGroupButton()

GroupButtons:AddButton({
    Title = "Remove Playtime Doors",
    Callback = function()
        local function sd(p)
            local o = game
            local r = p
            while r ~= '' do
                local d = r:find('.', 1, true)
                local n
                if d then
                    n = r:sub(1, d - 1)
                    r = r:sub(d + 1)
                else
                    n = r
                    r = ''
                end
                o = o:FindFirstChild(n)
                if not o then return end
            end
            if o then o:Destroy() end
        end

        sd("Workspace.Playtime.PlaytimeModel.PlaytimeRewards.Doors.Door1")
        sd("Workspace.Playtime.PlaytimeModel.PlaytimeRewards.Doors.Door2")
        sd("Workspace.Playtime.PlaytimeModel.PlaytimeRewards.Doors.Door3")
        sd("Workspace.Playtime.PlaytimeModel.PlaytimeRewards.Doors.Door4")
        sd("Workspace.Playtime.PlaytimeModel.PlaytimeRewards.Doors.Door")
    end,
})
