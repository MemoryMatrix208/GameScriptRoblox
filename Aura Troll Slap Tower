local Options = {}
for k in pairs(Modes) do
    table.insert(Options, k)
end
table.sort(Options)

MainSection:AddDropdown("EpicDropDownName", {
    Title = "Auto Win Mode",
    Description = "",
    Options = Options,
    Default = "",
    PlaceHolder = "Search Mode",
    Multiple = false,
    Callback = function(choice)
        local action = Modes[choice]
        if action then
            action()
        end
    end
})

local Main = main:AddTab("Aura")
main:SelectTab()

local MainSection = Main:AddSection({Title = "Aura Troll Slap Tower", Description = "Description", Defualt = false , Locked = false})
local GroupButtons =   MainSection:AddGroupButton()

GroupButtons:AddButton({
    Title = "+50 Aura",
    Callback = function()
        local plr = game:GetService("Players").LocalPlayer
        local char = plr.Character or plr.CharacterAdded:Wait()
        local hrp = char:WaitForChild("HumanoidRootPart")
        hrp.CFrame = CFrame.new(Vector3.new(438.836, 5.899, 237.585))
    end,
})

local Main = main:AddTab("Item")
main:SelectTab()

local MainSection = Main:AddSection({Title = "Aura Troll Slap Tower", Description = "Description", Defualt = false , Locked = false})
local GroupButtons =   MainSection:AddGroupButton()

local Players = game:GetService("Players")
local Player = Players.LocalPlayer

local function HRP()
    local c = Player.Character or Player.CharacterAdded:Wait()
    return c:WaitForChild("HumanoidRootPart")
end

local Points = {
    ["UltraCoil"] = Vector3.new(61.074, 19.564, -146.283),
    ["MedusaHead"] = Vector3.new(433.740, 4.864, 251.126),
}

MainSection:AddDropdown("EpicDropDownName", {
    Title = "Item",
    Description = "",
    Options = {"UltraCoil", "MedusaHead"},
    Default = "",
    PlaceHolder = "Search",
    Multiple = false,
    Callback = function(choice)
        local pos = Points[choice]
        if pos then
            HRP().CFrame = CFrame.new(pos)
        end
    end
})
