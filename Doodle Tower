local Main = main:AddTab("Item")
main:SelectTab()

local MainSection = Main:AddSection({Title = "Doodle Tower", Description = "Description", Defualt = false , Locked = false})
local GroupButtons =   MainSection:AddGroupButton()

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local function HRP()
    local c = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
    return c:WaitForChild("HumanoidRootPart")
end

local Positions = {
    ["BodySwapPotion"] = Vector3.new(548.590, 11.784, -183.797),
    ["GlobeLauncher"] = Vector3.new(543.635, 5.123, 455.780),
    ["FreeTool2"] = Vector3.new(457.430, 3.867, 401.815),
    ["HyperlaserGun"] = Vector3.new(121.604, 4.134, 97.991),
    ["Taser"] = Vector3.new(440.355, 183.998, -383.776),
}

MainSection:AddDropdown("EpicDropDownName", {
    Title = "Pick Tool",
    Description = "",
    Options = {
        "BodySwapPotion",
        "GlobeLauncher",
        "FreeTool2",
        "HyperlaserGun",
        "Taser",
    },
    Default = "",
    PlaceHolder = "Select Tool",
    Multiple = false,

    Callback = function(choice)
        local pos = Positions[choice]
        if pos then
            HRP().CFrame = CFrame.new(pos)
        end
    end
})

local Main = main:AddTab("Playtime Rewards")
main:SelectTab()

local MainSection = Main:AddSection({Title = "Doodle Tower", Description = "Description", Defualt = false , Locked = false})
local GroupButtons =   MainSection:AddGroupButton()

GroupButtons:AddButton({
    Title = "Remove Door",
    Callback = function()
local function sd(p)local o=game local r=p while r~='' do local d=r:find('.',1,true) local n if d then n=r:sub(1,d-1) r=r:sub(d+1) else n=r r='' end o=o:FindFirstChild(n) if not o then return end end if o then o:Destroy() end end
sd("Workspace.TimeDoorModel.TimeDoor")
    end,
  })
  
local Main = main:AddTab("Cash Drop")
main:SelectTab()

local MainSection = Main:AddSection({Title = "Doodle Tower", Description = "Description", Defualt = false , Locked = false})
local GroupButtons =   MainSection:AddGroupButton()

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local LocalPlayer = Players.LocalPlayer

local looping = false

local speedDelay = {
    Normal = 0.4,
    Fast = 0.2,
    ["Super Fast"] = 0.1,
    ["Speed of light"] = 0,
    Stable = 0.5,
    ["Debug Mode"] = 1,

    Lightning = 0,
    ["Hyper Speed"] = 0,
    Quantum = 0,
    Instant = 0,
    Immediate = 0,
    Rapid = 0.05,
    Swift = 0.1,
    Quick = 0.15,
    Express = 0.08,
    Blitz = 0,
    Flash = 0,
    Sonic = 0,
    Turbo = 0,
    Extreme = 0,
    Ultimate = 0,
    Maximum = 0,
    Optimal = 0.25,
    Balanced = 0.35,
    Conservative = 0.6,
    Minimal = 0.8,
    Lite = 0.7,
    Standard = 0.4,
    Premium = 0.25,
    Professional = 0.15,
    Enterprise = 0.1,
    ["Pro Precision"] = 0.05,
    ["Pro Optimized"] = 0.05,
    ["Extreme Burst"] = 0,
    ["Extreme No Delay"] = 0,
}

local function HRP()
    local c = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
    return c:WaitForChild("HumanoidRootPart")
end

local function touchAll(mode)
    local cashFolder = workspace:FindFirstChild("CashDrops")
    if not cashFolder then return end

    local cash = cashFolder:FindFirstChild("Cash")
    if not cash then return end

    for _,v in ipairs(cash:GetDescendants()) do
        if v:IsA("BasePart") then
            firetouchinterest(HRP(), v, 0)
            firetouchinterest(HRP(), v, 1)

            local d = speedDelay[mode]
            if d == 0 then
                RunService.Heartbeat:Wait()
            else
                task.wait(d or 0.3)
            end
        end
    end
end

local function startLoop(mode)
    looping = true
    task.spawn(function()
        while looping do
            touchAll(mode)
        end
    end)
end

local function stopAll()
    looping = false
end

