local lib = loadstring(game:HttpGet("https://raw.githubusercontent.com/WhoGodsyoure/Home/refs/heads/main/%E4%BA%BA%E7%94%9F%E3%81%AE%E6%84%8F%E5%91%B3%E3%81%A8%E3%81%AF%E4%BD%95%E3%81%8B"))() 
local FlagsManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/WhoGodsyoure/Home/refs/heads/main/Configure"))()

local main = lib:Load({
      Title = 'ArchiveXNova',
      ToggleButton = "rbxassetid://85025739024001",
	  BindGui = Enum.KeyCode.RightControl,
  })

local Main = main:AddTab("Chest")
main:SelectTab()

local MainSection = Main:AddSection({Title = "Sail Down A River", Description = "Description", Defualt = false , Locked = false})
local GroupButtons =   MainSection:AddGroupButton()

local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")

local LocalPlayer = Players.LocalPlayer
local StartPart = workspace:WaitForChild("RoundStartPart")
local TargetCFrame = CFrame.new(3.643, -2037.670, -5457.496)

local loop = false
local delayTime = 0.4
local loopCount = math.huge

local SpeedMap = {
    ["Normal"] = 0.6,
    ["Fast"] = 0.35,
    ["Super Fast"] = 0.18,
    ["Speed of light"] = 0.05,
    ["Stable"] = 0.7,
    ["Safe Mode"] = 1,
    ["Smart Speed"] = 0.25,
    ["Godspeed"] = 0.03,
    ["Overdrive"] = 0.02,
    ["Limit Break"] = 0.01,
    ["Low CPU"] = 1.2,
    ["FPS Friendly"] = 0.9,
    ["Throttled Mode"] = 1.5,
    ["Mobile Friendly"] = 1.1
}

local function getHRP()
    local char = LocalPlayer.Character
    return char and char:FindFirstChild("HumanoidRootPart")
end

local function tweenTo(cf)
    local hrp = getHRP()
    if not hrp then return end
    local tween = TweenService:Create(
        hrp,
        TweenInfo.new(math.max(delayTime, 0.01), Enum.EasingStyle.Linear),
        {CFrame = cf}
    )
    tween:Play()
    tween.Completed:Wait()
end

local function teleportSequence()
    tweenTo(StartPart.CFrame)
    tweenTo(TargetCFrame)
end

local function stopAll()
    loop = false
    loopCount = math.huge
end

local function startLoop()
    stopAll()
    loop = true
    task.spawn(function()
        local count = 0
        while loop and count < loopCount do
            pcall(teleportSequence)
            count += 1
            task.wait(delayTime)
        end
        loop = false
    end)
end

MainSection:AddDropdown("EpicDropDownName", {
    Title = "Teleport Mode",
    Description = "",
    PlaceHolder = "Select Mode",
    Multiple = false,
    Options = {

        "Just once | Normal",
        "Just once | Fast",
        "Just once | Super Fast",
        "Just once | Speed of light",
        "Just once | Stable",
        "Just once | Debug Mode",

        "Loop | Fast",
        "Loop | Super Fast",
        "Loop | Speed of light",
        "Loop | Stable",
        "Loop | Safe Mode",
        "Loop | Smart Speed",
        "Loop | X Times",
        "Loop | Timed (30s)",
        "Loop | Timed (1m)",
        "Loop | Timed (5m)",
        "Loop | Retry on Fail",
        "Loop | Auto Recover",
        "Loop | Pause on Error",
        "Loop | Cooldown Mode",
        "Loop | Burst Mode",
        "Loop | Priority High",
        "Loop | Priority Normal",
        "Loop | Priority Low",
        "Loop | Godspeed",
        "Loop | Overdrive",
        "Loop | Limit Break",
        "Stop Loop",

        "Auto | Adaptive Speed",
        "Auto | Smart Mode",
        "Auto | Context Aware",
        "Auto | Auto Pause",

        "Performance | Performance Based",
        "Performance | Low CPU",
        "Performance | FPS Friendly",
        "Performance | Throttled Mode",
        "Performance | Auto Optimize",
        "Performance | Mobile Friendly",

        "Farm | Efficient",
        "Farm | Aggressive",
        "Farm | AFK Friendly",
        "Farm | Background Mode",
        "Farm | Safe Farm",
        "Farm | Auto Resume",
        "Farm | Auto Pause",
        "Farm | Smart Targeting",
        "Stop Farm",

        "System | Emergency Stop",
        "System | Soft Reset",
        "System | Hard Reset",
        "System | Status Monitor",
        "System | Mode Indicator"
    },

    Callback = function(v)
        stopAll()

        if v == "Stop Loop" or v == "Stop Farm" or v == "System | Emergency Stop" then
            return
        end

        local _, speed = v:match("(.+) %| (.+)")
        if speed and SpeedMap[speed] then
            delayTime = SpeedMap[speed]
        end

        if v:find("Timed %(30s%)") then
            loopCount = math.floor(30 / delayTime)
        elseif v:find("Timed %(1m%)") then
            loopCount = math.floor(60 / delayTime)
        elseif v:find("Timed %(5m%)") then
            loopCount = math.floor(300 / delayTime)
        end

        if v:find("Just once") then
            teleportSequence()
        elseif v:find("Loop") or v:find("Auto") or v:find("Performance") or v:find("Farm") then
            startLoop()
        end
    end
})
