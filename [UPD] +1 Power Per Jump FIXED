local Main = main:AddTab("Rebirth")
main:SelectTab()

local MainSection = Main:AddSection({Title = "[UPD] +1 Power Per Jump", Description = "Description", Defualt = false , Locked = false})
local GroupButtons =   MainSection:AddGroupButton()

local ReplicatedStorage = game:GetService("ReplicatedStorage")

local Event = ReplicatedStorage
    .Modules.Packages._Index["sleitnick_net@0.2.0"]
    .net["RF/Rebirth"]

local rebirthLoop = false
local loopTask

local function rebirthOnce()
    Event:InvokeServer()
end

local function startLoop()
    if rebirthLoop then return end
    rebirthLoop = true

    loopTask = task.spawn(function()
        while rebirthLoop do
            Event:InvokeServer()
            task.wait()
        end
    end)
end

local function stopLoop()
    rebirthLoop = false
    if loopTask then
        task.cancel(loopTask)
        loopTask = nil
    end
end

MainSection:AddDropdown("EpicDropDownName", {
    Title = "Rebirth Mode",
    Description = "",
    Options = {
        "Normal (Once)",
        "Loop",
        "Stop Loop"
    },
    Default = "",
    PlaceHolder = "Select Mode",
    Multiple = false,

    Callback = function(mode)
        if mode == "Normal (Once)" then
            rebirthOnce()

        elseif mode == "Loop" then
            startLoop()

        elseif mode == "Stop Loop" then
            stopLoop()
        end
    end
})
