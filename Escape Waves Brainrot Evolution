local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/WhoGodsyoure/MyHomeObf/refs/heads/main/Meow"))()

local Window = Library:CreateWindow({
    Title = "Escape Waves Brainrot Evolution | ArchiveXNova",
    Accent = Color3.fromRGB(255, 100, 100)
})

local Main = Window:Section("Win")

local Players = game:GetService("Players")
local Player = Players.LocalPlayer

local function tp(pos)
    local char = Player.Character or Player.CharacterAdded:Wait()
    char:WaitForChild("HumanoidRootPart").CFrame = CFrame.new(pos)
end


local baseLoop = false

Main:Button("Auto Base Once", function()
    tp(Vector3.new(10.112, 85.607, -1088.293))
end)

Main:Button("Auto Base Loop", function()
    baseLoop = true
    task.spawn(function()
        while baseLoop do
            tp(Vector3.new(10.112, 85.607, -1088.293))
            task.wait()
        end
    end)
end)

Main:Button("Auto Base Stop", function()
    baseLoop = false
end)


local skyLoop = false

Main:Button("Auto Sky Once", function()
    tp(Vector3.new(411.460, 82.587, -1085.286))
