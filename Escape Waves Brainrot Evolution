local Main = Window:Section("Map")

Main:Button("Base", function()
    tp(Vector3.new(2.219, 8.198, 107.364))
end)

Main:Button("Sky", function()
    tp(Vector3.new(400.500, 8.198, 122.933))
end)

Main:Button("Twilight", function()
    tp(Vector3.new(805.638, 8.198, 125.440))
end)

Main:Button("Frozen", function()
    tp(Vector3.new(1203.493, 8.198, 125.277))
end)

local RunService = game:GetService("RunService")

local hatchConn

local function handleHatch(egg)
    if hatchConn then
        hatchConn:Disconnect()
        hatchConn = nil
    end

    local mode = 1

    if _G.HatchMode == "Once" then
        game:GetService("ReplicatedStorage").Packages._Index["acecateer_knit@1.7.2"].knit.Services.EggsService.RF.Hatch:InvokeServer(egg, 1)

    elseif _G.HatchMode == "Loop" then
        hatchConn = RunService.Heartbeat:Connect(function()
            game:GetService("ReplicatedStorage").Packages._Index["acecateer_knit@1.7.2"].knit.Services.EggsService.RF.Hatch:InvokeServer(egg, 1)
        end)

    elseif _G.HatchMode == "Stop" then
        if hatchConn then
            hatchConn:Disconnect()
            hatchConn = nil
        end
    end
end

Main:Button("Auto Buy Eggs Basic In Base Worlds (Win 1)", function()
    _G.HatchMode = _G.HatchMode == "Once" and "Loop" or _G.HatchMode == "Loop" and "Stop" or "Once"
    handleHatch("Basic")
end)

Main:Button("Auto Buy Eggs Troll In Base Worlds (Win 2)", function()
    _G.HatchMode = _G.HatchMode == "Once" and "Loop" or _G.HatchMode == "Loop" and "Stop" or "Once"
    handleHatch("Troll")
end)

