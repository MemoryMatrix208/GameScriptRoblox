local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/WhoGodsyoure/Home/refs/heads/main/%E3%82%A2%E3%83%83%E3%83%A9%E3%83%BC%E4%BB%A5%E5%A4%96%E3%81%AB%E7%A5%9E%E3%81%AF%E3%81%84%E3%81%AA%E3%81%84"))()

local Window = Library:CreateWindow('ArchiveXNova') -- :CreateWindow(Title)

local Section = Window:Section('Clone Troll Tower') -- :Section(Title)

local loopClone = false
local loopConn

Section:Button("Loop Clone", function()
    loopClone = not loopClone

    if loopClone then
        loopConn = task.spawn(function()
            while loopClone do
                local plr = game.Players.LocalPlayer
                local char = plr.Character
                if char then
                    local hrp = char:FindFirstChild("HumanoidRootPart")
                    local hum = char:FindFirstChildOfClass("Humanoid")

                    if hrp then
                        hrp.CFrame = CFrame.new(Vector3.new(-352.962, 510.872, -149.670))
                    end

                    if hum then
                        hum.Health = 0
                    end
                end
                task.wait()
            end
        end)
    else
        loopClone = false
    end
end)

Section:Label('Subscribe') -- :Label(Text)
