local Main = Window:Section("Player")

Main:Slider("Speed", 16, 1000, 50, 1, function(value)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = value
end)

Main:Slider("Jump", 50, 1000, 50, 1, function(value)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = value
end)
