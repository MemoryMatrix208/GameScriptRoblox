local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/WhoGodsyoure/MyHomeObf/refs/heads/main/Meow"))()

local Window = Library:CreateWindow({
    Title = "Escape Snow For Brainrots | ArchiveXNova",
    Accent = Color3.fromRGB(255, 100, 100)
})

local Main = Window:Section("Brainrots")

Main:Button("Auto God & Secret", function()

    local player = game.Players.LocalPlayer
    local char = player.Character or player.CharacterAdded:Wait()
    local hrp = char:WaitForChild("HumanoidRootPart")

    hrp.CFrame = CFrame.new(-727.478, 537.953, -2216.412)

    task.wait(1)

    local god = workspace:WaitForChild("ItemSpawners"):WaitForChild("God")
    local item = god:FindFirstChild("SpawnedItem")

    if item then
        for _, v in pairs(item:GetDescendants()) do
            if v:IsA("ProximityPrompt") then
                fireproximityprompt(v)
                break
            end
        end
    end

    task.wait(1)

    hrp.CFrame = CFrame.new(-690.287, 247.931, 347.817)

end)

local Main = Window:Section("Money")

local mode = 1
local collecting = false

local function collectSlots()
    local player = game.Players.LocalPlayer
    local char = player.Character or player.CharacterAdded:Wait()
    local hrp = char:WaitForChild("HumanoidRootPart")

    local plot = workspace:FindFirstChild("Plot_" .. player.Name)
    if not plot then return end

    for _, floor in pairs(plot:GetChildren()) do
        if floor.Name:match("Floor") then
            local slots = floor:FindFirstChild("Slots")
            if slots then
                for _, slot in pairs(slots:GetChildren()) do
                    local collect = slot:FindFirstChild("CollectTouch", true)
                    if collect and collect:IsA("BasePart") then
                        firetouchinterest(hrp, collect, 0)
                        firetouchinterest(hrp, collect, 1)
                        task.wait()
                    end
                end
            end
        end
    end
end

Main:Button("Auto Collect", function()

    if mode == 1 then
        collectSlots()
        mode = 2

    elseif mode == 2 then
        if collecting then return end
        collecting = true

        task.spawn(function()
            while collecting do
                collectSlots()
                task.wait(0.5)
            end
        end)

        mode = 3

    elseif mode == 3 then
        collecting = false
        mode = 1
    end

end)

local Main = Window:Section("Upgrade Shop")

Main:Button("Purchase Speed +1", function()
    game:GetService("ReplicatedStorage").Events.PurchaseSpeed:FireServer(1)
end)

Main:Button("Purchase Speed +5", function()
    game:GetService("ReplicatedStorage").Events.PurchaseSpeed:FireServer(5)
end)

Main:Button("Purchase Speed +10", function()
    game:GetService("ReplicatedStorage").Events.PurchaseSpeed:FireServer(10)
end)

Main:Button("Purchase Carry", function()
    game:GetService("ReplicatedStorage").Events.PurchaseCarry:FireServer()
end)

Main:Button("Purchase Base Upgrade", function()
    game:GetService("ReplicatedStorage").Events.RequestBaseUpgrade:FireServer()
end)

local Main = Window:Section("Rebirth")

Main:Button("Click", function()
    game:GetService("ReplicatedStorage").Events.RequestRebirth:FireServer()
end)

local Main = Window:Section("Inventory")

Main:Button("Sell Inventory", function()
    game:GetService("ReplicatedStorage").Events.RequestSell:FireServer("Inventory")
end)

local Main = Window:Section("VIP")

Main:Button("Remove VIP", function()
    local vip = workspace:FindFirstChild("Container")
    if vip and vip:FindFirstChild("VIP") then
        vip.VIP:Destroy()
    end
end)

local Main = Window:Section("Waves")

Main:Button("Remove Waves", function()
    local waves = workspace:FindFirstChild("Waves")
    if waves then
        waves:Destroy()
    end
end)

local Main = Window:Section("Map")

Main:Button("TP Home", function()
    local player = game.Players.LocalPlayer
    local char = player.Character or player.CharacterAdded:Wait()
    local hrp = char:WaitForChild("HumanoidRootPart")
    hrp.CFrame = CFrame.new(-690.287, 247.931, 347.817)
end)

Main:Button("TP God And Secret", function()
