local Main = Window:Section("Win")

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local TARGET_POS = Vector3.new(-4.665, -60.503, 3017.115)

local running = false

local function getRoot()
    local char = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
    return char:WaitForChild("HumanoidRootPart")
end

local function execute()
    local root = getRoot()
    root.CFrame = CFrame.new(TARGET_POS)
end

Main:Button("Auto Win | Once", function()
    execute()
end)

Main:Button("Auto Win | Loop", function()
    if running then return end
    running = true
    task.spawn(function()
        while running do
            execute()
            task.wait(1)
        end
    end)
end)

Main:Button("Stop", function()
    running = false
end)

local Main = Window:Section("Money")

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local running = false

local function getRoot()
    local char = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
    return char:WaitForChild("HumanoidRootPart")
end

local function execute()
    local root = getRoot()

    local plotName = "Plot_" .. LocalPlayer.Name
    local plot = workspace:FindFirstChild(plotName)
    if not plot then return end

    for f = 1, 6 do
        local floor = plot:FindFirstChild("Floor" .. f)
        if floor then
            local slots = floor:FindFirstChild("Slots")
            if slots then
                for s = 1, 10 do
                    local slot = slots:FindFirstChild("Slot" .. s)
                    if slot then
                        local touch = slot:FindFirstChild("CollectTouch")
                        if touch then
                            firetouchinterest(root, touch, 0)
                            task.wait(0.1)
                            firetouchinterest(root, touch, 1)
                            task.wait(0.3)
                        end
                    end
                end
            end
        end
    end
end

Main:Button("Auto Collect Money | Once", function()
    execute()
end)

Main:Button("Auto Collect Money | Loop", function()
    if running then return end
    running = true
    task.spawn(function()
        while running do
            execute()
            task.wait(2)
        end
    end)
end)

Main:Button("Stop", function()
    running = false
end)

local Main = Window:Section("Upgrade Shop")

local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Events = ReplicatedStorage:WaitForChild("Events")

local PurchaseSpeed = Events:WaitForChild("PurchaseSpeed")

Main:Button("Purchase Speed +1", function()
    PurchaseSpeed:FireServer(1)
end)

Main:Button("Purchase Speed +5", function()
    PurchaseSpeed:FireServer(5)
end)

Main:Button("Purchase Speed +10", function()
    PurchaseSpeed:FireServer(10)
end)

local BaseUpgrade = Events:WaitForChild("RequestBaseUpgrade")

Main:Button("Purchase Base Upgrade", function()
    BaseUpgrade:FireServer()
end)

local PurchaseCarry = Events:WaitForChild("PurchaseCarry")

Main:Button("Purchase Carry", function()
    PurchaseCarry:FireServer()
end)

local Main = Window:Section("Rebirth")

local Rebirth = Events:WaitForChild("RequestRebirth")

Main:Button("Click", function()
    Rebirth:FireServer()
end)

local Main = Window:Section("Inventory")

local Sell = Events:WaitForChild("RequestSell")

Main:Button("Sell Inventory", function()
    Sell:FireServer("Inventory")
end)

local Main = Window:Section("VIP")

Main:Button("Remove Door", function()
    local vip = workspace:FindFirstChild("VIP")
    if vip then
        vip:Destroy()
    end
end)

local Main = Window:Section("Tsunamis")

Main:Button("Remove Tsunamis", function()
    local tsu = workspace:FindFirstChild("Tsunamis")
    if tsu then
        tsu:Destroy()
    end
end)
