local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/WhoGodsyoure/MyHome/refs/heads/main/Made%20By%20Me%20Part%202%20Open%20And%20Close"))()

local Window = Library:CreateWindow({
    Title = "Escape Tsunami For Devil Fruits | ArchiveXNova"
})

local Main = Window:Section("Brainrot")

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local START_POS = Vector3.new(-9.646, -61.172, 3174.078)
local END_POS = Vector3.new(59.135, -61.501, 74.868)

local function getRoot()
    local char = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
    return char:WaitForChild("HumanoidRootPart")
end

Main:Button("Auto Pickup Celestial | Once", function()
    local root = getRoot()

    
    root.CFrame = CFrame.new(START_POS)
    task.wait(0.8)

    
    local spawnedItem = workspace
        :WaitForChild("ItemSpawners")
        :WaitForChild("Celestial")
        :WaitForChild("SpawnedItem")

    
    root.CFrame = spawnedItem:GetPivot()
    task.wait(0.8)

    
    for _, obj in ipairs(spawnedItem:GetDescendants()) do
        if obj:IsA("ProximityPrompt") then
            fireproximityprompt(obj)
            task.wait(0.5)
        end
    end

    task.wait(0.8)

    
    root.CFrame = CFrame.new(END_POS)
end)

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local START_POS = Vector3.new(-9.646, -61.172, 3174.078)
local END_POS = Vector3.new(59.135, -61.501, 74.868)

local running = false

local function getRoot()
    local char = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
    return char:WaitForChild("HumanoidRootPart")
end

local function execute()
    local root = getRoot()

    root.CFrame = CFrame.new(START_POS)
    task.wait(0.8)

    local spawnedItem = workspace
        :WaitForChild("ItemSpawners")
        :WaitForChild("Celestial")
        :WaitForChild("SpawnedItem")

    root.CFrame = spawnedItem:GetPivot()
    task.wait(0.8)

    for _, obj in ipairs(spawnedItem:GetDescendants()) do
        if obj:IsA("ProximityPrompt") then
            fireproximityprompt(obj)
            task.wait(0.5)
        end
    end

    task.wait(0.8)
    root.CFrame = CFrame.new(END_POS)
end

Main:Button("Auto Pickup Celestial | Loop", function()
    if running then return end
    running = true
    task.spawn(function()
        while running do
            execute()
            task.wait(3)
        end
    end)
end)

Main:Button("Stop", function()
    running = false
end)

