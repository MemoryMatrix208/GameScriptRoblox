local Main = main:AddTab("Rebirth")
main:SelectTab()

local MainSection = Main:AddSection({Title = "+1 Speed Car Tsunami Escape", Description = "Description", Defualt = false , Locked = false})
local GroupButtons =   MainSection:AddGroupButton()

local RunService = game:GetService("RunService")
local Event = game:GetService("ReplicatedStorage").Events.RequestRebirth

local loopConn

local function stopLoop()
    if loopConn then
        loopConn:Disconnect()
        loopConn = nil
    end
end

GroupButtons:AddButton({
    Title = "Rebirth | Once",
    Callback = function()
        stopLoop()
        Event:InvokeServer()
    end,
})

GroupButtons:AddButton({
    Title = "Rebirth | Loop",
    Callback = function()
        stopLoop()
        loopConn = RunService.Heartbeat:Connect(function()
            Event:InvokeServer()
            task.wait(0.3)
        end)
    end,
})

GroupButtons:AddButton({
    Title = "Rebirth | Stop Loop",
    Callback = function()
        stopLoop()
    end,
})

local Main = main:AddTab("Door")
main:SelectTab()

local MainSection = Main:AddSection({Title = "+1 Speed Car Tsunami Escape", Description = "Description", Defualt = false , Locked = false})
local GroupButtons =   MainSection:AddGroupButton()

GroupButtons:AddButton({
    Title = "Delete VIP Doors",
    Callback = function()
        local obj = workspace:FindFirstChild("vipDoors")
        if obj then
            obj:Destroy()
        end
    end,
})
