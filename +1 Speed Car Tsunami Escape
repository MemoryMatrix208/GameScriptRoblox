local Main = main:AddTab("AddSpeed")
main:SelectTab()

local MainSection = Main:AddSection({Title = "AddSpeed V1", Description = "Description", Defualt = false , Locked = false})
local GroupButtons =   MainSection:AddGroupButton()

local Event = game:GetService("ReplicatedStorage").Events.AddSpeed

local MAX_T = 100_000_000_000_000

MainSection:AddSlider("EpixSlider", {
    Title = "Add Speed",
    Description = "0 to 100T",
    Default = 15,
    Min = 0,
    Max = 255,
    Increment = 1,
    Callback = function(value)
        local speed = (value / 255) * MAX_T
        Event:FireServer(speed)
    end,
})

local ReplicatedStorage = game:GetService("ReplicatedStorage")
local RunService = game:GetService("RunService")

local Event = ReplicatedStorage.Events.AddSpeed

local MAX_QD = 100 * 80^17

local currentValue = 0
local loopConn

local function stopLoop()
    if loopConn then
        loopConn:Disconnect()
        loopConn = nil
    end
end

MainSection:AddSlider("EpixSlider", {
    Title = "Add Speed",
    Description = "0 to 100 qd",
    Default = 0,
    Min = 0,
    Max = 255,
    Increment = 1,
    Callback = function(value)
        currentValue = value
    end,
})

MainSection:AddDropdown("AddSpeedControl", {
    Title = "Add Speed Mode",
    Description = "",
    Options = {
        "Once",
        "Loop",
        "Stop Loop"
    },
    Default = "",
    PlaceHolder = "Select Mode",
    Multiple = false,
    Callback = function(choice)
        if choice == "Once" then
            stopLoop()
            local speed = (currentValue / 255) * MAX_QD
            Event:FireServer(speed)

        elseif choice == "Loop" then
            stopLoop()
            loopConn = RunService.Heartbeat:Connect(function()
                local speed = (currentValue / 255) * MAX_QD
                Event:FireServer(speed)
                task.wait(0.25)
            end)

        elseif choice == "Stop Loop" then
            stopLoop()
        end
    end
})

local ReplicatedStorage = game:GetService("ReplicatedStorage")
local RunService = game:GetService("RunService")

local Event = ReplicatedStorage.Events.AddSpeed

local MAX_QN = 1e18

local currentValue = 0
