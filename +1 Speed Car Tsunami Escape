local MainSection = Main:AddSection({Title = "AddSpeed V2", Description = "Description", Defualt = false , Locked = false})
local GroupButtons =   MainSection:AddGroupButton()

GroupButtons:AddButton({
    Title = "Add Speed 9.2 Qn (Exact)",
    Callback = function()
        local Event = game:GetService("ReplicatedStorage").Events.AddSpeed
        Event:FireServer(9200000000000000000)
    end,
})

