local FlagsManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/WhoGodsyoure/Home/refs/heads/main/Configure"))()

local main = lib:Load({
      Title = 'ArchiveXNova',
      ToggleButton = "rbxassetid://85025739024001",
	  BindGui = Enum.KeyCode.RightControl,
  })

local Main = main:AddTab("Coin")
main:SelectTab()

local MainSection = Main:AddSection({Title = "Find the Slaps Tower", Description = "Description", Defualt = false , Locked = false})
local GroupButtons =   MainSection:AddGroupButton()

MainSection:AddDropdown("EpicDropDownName", {
    Title = "Coin",
    Description = "",
    Options = {
        "+500",
        "+750",
        "+1000",
        "+1250",
        "+1500"
    },
    Default = "",
    PlaceHolder = "Search Coin",
    Multiple = false,
    Callback = function(balls)
        local map = {
            ["+500"]  = Vector3.new(-294.331, 5.000, -3.570),
            ["+750"]  = Vector3.new(-75.246, 66.301, -245.000),
            ["+1000"] = Vector3.new(-49.283, 6.521, -372.372),
            ["+1250"] = Vector3.new(49.449, 225.000, -53.704),
            ["+1500"] = Vector3.new(132.968, 288.000, 21.895)
        }

        local pos = map[balls]
        if pos then
            game.Players.LocalPlayer.Character:PivotTo(CFrame.new(pos))
        end
    end
})

local Main = main:AddTab("Win")
main:SelectTab()

local MainSection = Main:AddSection({Title = "Find the Slaps Tower", Description = "Description", Defualt = false , Locked = false})
local GroupButtons =   MainSection:AddGroupButton()

local Players = game:GetService("Players")
local lp = Players.LocalPlayer
local char = lp.Character or lp.CharacterAdded:Wait()
local hrp = char:WaitForChild("HumanoidRootPart")

local target = workspace:WaitForChild("WinnersPart")

local looping = false
local loopDelay = 0.1

local function touchOnce()
    firetouchinterest(hrp, target, 0)
    firetouchinterest(hrp, target, 1)
end

local function startLoop()
    if looping then return end
    looping = true
    task.spawn(function()
        while looping do
            touchOnce()
            task.wait(loopDelay)
        end
    end)
end

local function stopLoop()
    looping = false
end

MainSection:AddDropdown("EpicDropDownName", {
    Title = "Teleport",
    Description = "",
    Options = {
