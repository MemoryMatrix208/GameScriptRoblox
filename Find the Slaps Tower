        "Auto | Extreme Autonomous",
        "Auto | Extreme Self Running",

        "Performance | Performance Based",
        "Performance | Low CPU",
        "Performance | FPS Friendly",
        "Performance | Throttled Mode",
        "Performance | Auto Optimize",
        "Performance | Mobile Friendly",
        "Performance | Pro Balance",
        "Performance | Extreme Performance",

        "Farm | Efficient",
        "Farm | Aggressive",
        "Farm | AFK Friendly",
        "Farm | Background Mode",
        "Farm | Safe Farm",
        "Farm | Auto Resume",
        "Farm | Auto Pause",
        "Farm | Smart Targeting",
        "Farm | Pro Farming",
        "Farm | Extreme Farming",
        "Stop Farm",

        "System | Emergency Stop",
        "System | Soft Reset",
        "System | Hard Reset",
        "System | Status Monitor",
        "System | Mode Indicator",
        "System | Pro Control",
        "System | Extreme Control"
    },
    Default = "",
    PlaceHolder = "Search Mode",
    Multiple = false,
    Callback = function(choice)
        if string.find(choice, "Just once") then
            stopLoop()
            touchOnce()

        elseif string.find(choice, "Loop") then
            if string.find(choice, "Super Fast") then
                loopDelay = 0.03
            elseif string.find(choice, "Speed of light") then
                loopDelay = 0
            else
                loopDelay = 0.1
            end
            startLoop()

        elseif choice == "Stop Loop" or choice == "System | Emergency Stop" or choice == "Stop Farm" then
            stopLoop()
        end
    end
})

local Main = main:AddTab("Slash")
main:SelectTab()

local MainSection = Main:AddSection({Title = "Find the Slaps Tower", Description = "Description", Defualt = false , Locked = false})
local GroupButtons =   MainSection:AddGroupButton()

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local function getEvent()
    local char = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
    return char:WaitForChild("Slap"):WaitForChild("Event")
end

local loopAttack = false

MainSection:AddDropdown("EpicDropDownName", {
    Title = "Slap All Players",
    Description = "",
    Options = {
        "Normal (Once)",
        "Loop",
        "Stop Loop"
    },
    Default = "",
    PlaceHolder = "Select Mode",
    Multiple = false,
    Callback = function(mode)

        local Event = getEvent()

        if mode == "Normal (Once)" then
            for _, plr in ipairs(Players:GetPlayers()) do
                if plr ~= LocalPlayer and plr.Character then
                    Event:FireServer(
                        "slash",
                        plr.Character,
                        Vector3.new(999, 999, 999)
                    )
                end
            end

        elseif mode == "Loop" then
            loopAttack = true
            task.spawn(function()
                while loopAttack do
                    for _, plr in ipairs(Players:GetPlayers()) do
                        if plr ~= LocalPlayer and plr.Character then
                            Event:FireServer(
                                "slash",
                                plr.Character,
                                Vector3.new(999, 999, 999)
                            )
                        end
                    end
                    task.wait(0.1)
                end
            end)

        elseif mode == "Stop Loop" then
            loopAttack = false
        end
    end
})
