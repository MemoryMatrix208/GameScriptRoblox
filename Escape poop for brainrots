local main = window:Section("Sell All Brainrot | Made By ArchiveXNova")

main:Button("Sell All After that Rejoin", function()
    local Event = game:GetService("ReplicatedStorage").Remote.SellF
    Event:InvokeServer("SellAll")
end)

local main = window:Section("Upgrade Base | Made By ArchiveXNova")

main:Button("Upgrade Once Base", function()
    local Event = game:GetService("ReplicatedStorage").Remote.BaseUpgrade
    Event:FireServer("Upgrade")
end)

local main = window:Section("Upgrade Shop | Made By ArchiveXNova")

main:Button("Upgrade Once Carry Up", function()
    local Event = game:GetService("ReplicatedStorage").Remote.Player
    Event:FireServer("CarryUp")
end)

main:Button("Upgrade Once Speed x10", function()
    local Event = game:GetService("ReplicatedStorage").Remote.Speed
    Event:FireServer("UpgradeSpeed", 10)
end)

local main = window:Section("VIP | Made By ArchiveXNova")

main:Button("Remove VIP Walls", function()
    local wall = workspace:FindFirstChild("\232\181\155\233\129\147")
    if wall and wall:FindFirstChild("VIPWalls") then
        wall.VIPWalls:Destroy()
    end
end)
