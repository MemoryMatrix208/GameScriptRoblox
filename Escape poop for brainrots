local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/WhoGodsyoure/MyHome/refs/heads/main/Made%20By%20Me%20Part%201%20Open%20And%20Close"))()

local window = Library:CreateWindow({
    Title = "Escape poop for brainrots"
})

local main = window:Section("Brainrot | Secret | Made By ArchiveXNova")

main:Button("Auto Pick up Secret Once", function()
    local Players = game:GetService("Players")
    local player = Players.LocalPlayer

    local TARGET_CFRAME = CFrame.new(47.523, 6.197, -19.134)

    local function HRP()
        local char = player.Character or player.CharacterAdded:Wait()
        return char:WaitForChild("HumanoidRootPart")
    end

    local decoFolder = workspace
        :WaitForChild("GameObjectPool")
        :WaitForChild("Decoration")
        :FindFirstChild("8")

    if not decoFolder then return end

    
    local basePart = decoFolder:FindFirstChildWhichIsA("BasePart", true)
    if not basePart then return end

    
    HRP().CFrame = basePart.CFrame
    task.wait(0.4)

    
    local prompt = decoFolder:FindFirstChildWhichIsA("ProximityPrompt", true)
    if prompt then
        fireproximityprompt(prompt)
        task.wait(0.3)
    end

    
    HRP().CFrame = TARGET_CFRAME
end)

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")

local player = Players.LocalPlayer
local TARGET_CFRAME = CFrame.new(47.523, 6.197, -19.134)

local looping = false
local loopConn

local function HRP()
    local char = player.Character or player.CharacterAdded:Wait()
    return char:WaitForChild("HumanoidRootPart")
end

local function stopLoop()
    looping = false
    if loopConn then
        loopConn:Disconnect()
        loopConn = nil
    end
end

main:Button("Auto Pick up Secret | Loop And Use Anti afk", function()
    if looping then
        stopLoop()
        return
    end

    looping = true

    loopConn = RunService.Heartbeat:Connect(function()
        if not looping then return end

        local deco = workspace.GameObjectPool.Decoration:FindFirstChild("8")
        if not deco then return end

        local part = deco:FindFirstChildWhichIsA("BasePart", true)
        if not part then return end

        HRP().CFrame = part.CFrame
        task.wait(1)

        local prompt = deco:FindFirstChildWhichIsA("ProximityPrompt", true)
        if prompt then
            fireproximityprompt(prompt)
        end

        task.wait(1)
        HRP().CFrame = TARGET_CFRAME
        task.wait(1)
    end)
end)

