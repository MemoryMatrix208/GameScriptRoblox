    if loopConn then
        loopConn:Disconnect()
        loopConn = nil
    end
end

main:Button("Auto Pick up Celestial | Loop And Use Anti afk", function()
    if looping then
        stopLoop()
        return
    end

    looping = true

    loopConn = RunService.Heartbeat:Connect(function()
        if not looping then return end

        local deco = workspace.GameObjectPool.Decoration:FindFirstChild("9")
