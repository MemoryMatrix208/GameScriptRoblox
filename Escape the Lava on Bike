-- SPEED DROPDOWN
MainSection:AddDropdown("SpeedMode", {
    Title = "Speed Mode",
    Options = (function()
        local t = {}
        for k in pairs(SpeedLogic) do table.insert(t, k) end
        table.sort(t)
        return t
    end)(),
    PlaceHolder = "Select Speed Mode",
    Multiple = false,
    Callback = function(choice)
        StopLoop()
        if SpeedLogic[choice] then
            SpeedLogic[choice]()
        end
    end
})
