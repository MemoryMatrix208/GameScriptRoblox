local lib = loadstring(game:HttpGet("https://raw.githubusercontent.com/WhoGodsyoure/Home/refs/heads/main/%E4%BA%BA%E7%94%9F%E3%81%AE%E6%84%8F%E5%91%B3%E3%81%A8%E3%81%AF%E4%BD%95%E3%81%8B"))() 
local FlagsManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/WhoGodsyoure/Home/refs/heads/main/Configure"))()

local main = lib:Load({
      Title = 'ArchiveXNova',
      ToggleButton = "rbxassetid://85025739024001",
	  BindGui = Enum.KeyCode.RightControl,
  })

local Main = main:AddTab("Welcome")
main:SelectTab()

local MainSection = Main:AddSection({Title = "Escape the Lava on Bike", Description = "Description", Defualt = false , Locked = false})
local GroupButtons =   MainSection:AddGroupButton()

-- SERVICES
local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local RunService = game:GetService("RunService")
local Player = Players.LocalPlayer

-- CHARACTER
local function HRP()
    local c = Player.Character or Player.CharacterAdded:Wait()
    return c:WaitForChild("HumanoidRootPart")
end

-- WORLDS DATA
local Worlds = {
    ["World 1"] = Vector3.new(-6.179, 101.501, -1087.459),
    ["World 2"] = Vector3.new(303.259, 94.736, -1088.361),
    ["World 3"] = Vector3.new(664.428, 82.920, -1087.282),
    ["World 4"] = Vector3.new(1010.999, 86.472, -1086.176),
}

-- STATE
local TargetWorld = nil
local LoopConn = nil

-- CORE CONTROL
local function StopLoop()
    if LoopConn then
        LoopConn:Disconnect()
        LoopConn = nil
    end
end

-- TELEPORT CORE
local function Instant()
    if TargetWorld then
        HRP().CFrame = CFrame.new(TargetWorld)
    end
end

local function Tween(speed)
    if not TargetWorld then return end
    local d = (HRP().Position - TargetWorld).Magnitude
    TweenService:Create(
        HRP(),
        TweenInfo.new(d / speed, Enum.EasingStyle.Linear),
        {CFrame = CFrame.new(TargetWorld)}
    ):Play()
end

local function Loop(delayTime)
    StopLoop()
    LoopConn = RunService.Heartbeat:Connect(function()
        if TargetWorld then
            HRP().CFrame = CFrame.new(TargetWorld)
            task.wait(delayTime)
        end
    end)
end

-- WORLDS DROPDOWN
MainSection:AddDropdown("WorldsDropdown", {
    Title = "Worlds",
    Options = {"World 1","World 2","World 3","World 4"},
    PlaceHolder = "Select World",
    Multiple = false,
    Callback = function(choice)
        TargetWorld = Worlds[choice]
        Instant()
    end
})

